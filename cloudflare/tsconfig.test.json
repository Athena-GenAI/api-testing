{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "types": ["@cloudflare/workers-types", "jest", "node"],
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2021",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["workers/**/*", "**/*.test.ts"],
  "exclude": ["node_modules", "dist"]
}
